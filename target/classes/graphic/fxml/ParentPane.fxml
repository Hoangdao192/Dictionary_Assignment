<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="850.0" prefWidth="1300.0" stylesheets="@../css/ParentPane.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="graphic.MainController">
    <children>
        <AnchorPane id="side-bar" prefHeight="850.0" prefWidth="100.0">
         <children>
              <Button id="sidebar_search_button" layoutX="20.0" layoutY="130.0" mnemonicParsing="false" onAction="#onSearchButtonClick" prefHeight="60.0" prefWidth="60.0" styleClass="sidebar-button">
                  <graphic>
                      <ImageView fitHeight="31.0" fitWidth="31.0" pickOnBounds="true" preserveRatio="true">
                          <image>
                              <Image url="@../image/search.png" />
                          </image>
                      </ImageView>
                  </graphic>
              </Button>
            <Button id="sidebar_googletranslate_button" layoutX="20.0" layoutY="250.0" mnemonicParsing="false" onAction="#onGoogleTranslateButtonClick" prefHeight="60.0" prefWidth="60.0" styleClass="sidebar-button">
               <graphic>
                  <ImageView fitHeight="31.0" fitWidth="31.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../image/google_translate.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button id="sidebar_localdictionary_button" layoutX="20.0" layoutY="370.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="60.0" styleClass="sidebar-button">
               <graphic>
                  <ImageView fitHeight="31.0" fitWidth="31.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../image/local_dictionary.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children></AnchorPane>
      <AnchorPane fx:id="mainPane" layoutX="100.0" prefHeight="850.0" prefWidth="1200.0" />
    </children>
</AnchorPane>
