<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="container" fx:id="myPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="graphic.ModifyController">
    <children>
        <TableView id="table-view" fx:id="tableWord" layoutX="40.0" layoutY="80.0" prefHeight="280.0" prefWidth="400.0" AnchorPane.bottomAnchor="40.0" AnchorPane.topAnchor="80.0">
            <columns>
                <TableColumn id="search-box-find" fx:id="columnWord" prefWidth="400.0" />
            </columns>
        </TableView>
        <Button id="search-button" fx:id="buttonSearch" layoutX="480.0" layoutY="40.0" mnemonicParsing="false" onAction="#clickButtonSearch" prefHeight="62.0" prefWidth="62.0" AnchorPane.bottomAnchor="298.0" AnchorPane.leftAnchor="480.0" AnchorPane.topAnchor="40.0" />
        <Button id="new-button" fx:id="buttonNew" layoutX="480.0" layoutY="134.0" mnemonicParsing="false" onAction="#clickButtonNew" prefHeight="62.0" prefWidth="62.0" text="New" AnchorPane.leftAnchor="480.0" AnchorPane.topAnchor="120.0">
         <graphic>
            <ImageView fitHeight="25.0" fitWidth="1.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../../../Dictionary_Assignment/src/main/resources/graphic/image/search.png" />
               </image>
            </ImageView>
         </graphic></Button>
        <Button id="delete-button" fx:id="buttonDelete" layoutX="480.0" layoutY="220.0" mnemonicParsing="false" onAction="#clickButtonDelete" prefHeight="62.0" prefWidth="62.0" text="Delete" AnchorPane.leftAnchor="480.0" AnchorPane.topAnchor="220.0">
         <graphic>
            <ImageView fitHeight="44.0" fitWidth="1.0" pickOnBounds="true" preserveRatio="true" />
         </graphic></Button>
        <Button id="detail-button" fx:id="buttonDetail" layoutX="486.0" layoutY="322.0" mnemonicParsing="false" onAction="#clickButtonDetail" prefHeight="62.0" prefWidth="62.0" text="Detail" AnchorPane.leftAnchor="480.0" AnchorPane.topAnchor="310.0">
         <graphic>
            <ImageView fitHeight="20.0" fitWidth="1.0" pickOnBounds="true" preserveRatio="true" />
         </graphic></Button>
        <TextField id="word-button" fx:id="textSearch" layoutX="40.0" layoutY="40.0" prefHeight="36.0" prefWidth="400.0" promptText="Search World">
         <font>
            <Font name="Times New Roman Bold" size="12.0" />
         </font></TextField>
      <ImageView fx:id="imageView" fitHeight="46.0" fitWidth="46.0" layoutX="488.0" layoutY="48.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../../../Dictionary_Assignment/src/main/resources/graphic/image/search.png" />
         </image>
      </ImageView>
    </children>
</AnchorPane>
